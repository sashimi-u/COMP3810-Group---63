# Example environment variables for the project
# Copy this file to `models/.env` and fill in real values for production

# Server port
PORT=3000

# MongoDB connection string
MONGO_URL=mongodb://localhost:27017/taskdb

# Admin account (used by demo / ensure-admin scripts)
ADMIN_USERNAME=admin
ADMIN_PASSWORD=admin

# Cookie-session keys (comma-separated)
SESSION_KEYS=devkey1,devkey2

# Node environment
NODE_ENV=development
